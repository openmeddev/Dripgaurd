
//test alarm lights = LEDs

void setup()
{
  pinMode(30, OUTPUT);
  pinMode(31, OUTPUT);
  pinMode(32, OUTPUT);
  pinMode(33, OUTPUT);
  pinMode(34, OUTPUT);
  pinMode(35, OUTPUT);
  pinMode(36, OUTPUT);
  pinMode(37, OUTPUT);
}
void loop()
{
  digitalWrite(30, HIGH);
  delay(500);
  digitalWrite(30, LOW);
  delay(500);
  digitalWrite(31, HIGH);
  delay(500);
  digitalWrite(31, LOW);
  delay(500);
  digitalWrite(32, HIGH);
  delay(500);
  digitalWrite(32, LOW);
  delay(500);
  digitalWrite(33, HIGH);
  delay(500);
  digitalWrite(33, LOW);
  delay(500);
  digitalWrite(34, HIGH);
  delay(500);
  digitalWrite(34, LOW);
  delay(500);
  digitalWrite(35, HIGH);
  delay(500);
  digitalWrite(35, LOW);
  delay(500);
  digitalWrite(36, HIGH);
  delay(500);
  digitalWrite(36, LOW);
  delay(500);
  digitalWrite(37, HIGH);
  delay(500);
  digitalWrite(37, LOW);
  delay(500);
}

